<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @comercios.each do |comercio| %>
      <tr>
        <td><%= comercio.nombre %></td>
        <td><%= comercio.descripcion %></td>
        <td><%= link_to 'Show', comercio %></td>
        <td><%= link_to 'Edit', edit_comercio_path(comercio) %></td>
        <td><%= link_to 'Destroy', comercio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

